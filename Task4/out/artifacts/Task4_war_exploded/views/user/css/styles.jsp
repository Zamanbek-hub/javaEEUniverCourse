ul {
    padding-left: 0;
    list-style-type: none;
}

li{
    padding-top: 10px;
}

textarea{
    min-height: 100px;
    max-height: 200px;
    min-width: 200px;
}

td{
    max-width: 500px;
}

a{
    color:white;
}

header{
    margin-top: 50px;
    width:100%;
    height: 200px;
    background-color: #68B5F9;
    border-radius: 8px;
    color:white;
}

header .header_div{
    padding-top: 30px;
    height: 80%;
    width: 80%;
    display: block;
    margin-left: auto;
    margin-right: auto;

}

header .header_div .header_title{
    font-weight: initial;
    font-style:italic;
    font-size: 80px;
}